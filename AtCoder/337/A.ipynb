    "N = int(input())\n",
    "T, A = 0, 0\n",
    "for i in range(N):\n",
    "    t, a = map(int, input().split())\n",
    "    T += t\n",
    "    A += a\n",
    "\n",
    "if T > A:\n",
    "    print('Takahashi')\n",
    "elif A > T:\n",
    "    print('Aoki')\n",
    "else:\n",
    "    print('Draw')"
   ]
